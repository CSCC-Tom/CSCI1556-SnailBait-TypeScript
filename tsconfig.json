{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 18",

  "_version": "18.2.0",

  "compilerOptions": {
    "lib": ["es2023", "DOM"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "sourceMap": true,
    "target": "es2022",
    "outDir": "_compiled",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", "_compiled"]
}
